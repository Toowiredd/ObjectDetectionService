datasource db {
  provider = "mysql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model AluminiumCan {
  confidenceScore    Float?
  count              Int?
  createdAt          DateTime  @default(now())
  detectionTimestamp DateTime?
  id                 String    @id @default(cuid())
  typeField          String?
  updatedAt          DateTime  @updatedAt
}

model Hdpe2PlasticBottle {
  confidenceScore    Float?
  count              Int?
  createdAt          DateTime  @default(now())
  detectionTimestamp DateTime?
  id                 String    @id @default(cuid())
  typeField          String?
  updatedAt          DateTime  @updatedAt
}

